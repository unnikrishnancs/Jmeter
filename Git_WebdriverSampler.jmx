            <stringProp name="WebDriverSampler.script">var pkg=JavaImporter(org.openqa.selenium)
var support_ui = JavaImporter(org.openqa.selenium.support.ui.WebDriverWait)
var wait = new support_ui.WebDriverWait(WDS.browser, 5000) //Timeout

//Launch the page
WDS.sampleResult.subSampleStart(&quot;01_UI_PageLoad&quot;)
WDS.browser.get(&apos;https://blazedemo.com/&apos;)
//Validate page
if(WDS.browser.getTitle()!=&apos;BlazeDemo&apos;)
{
	 WDS.sampleResult.subSampleEnd(false)	 
	 WDS.sampleResult.setSuccessful(false)	 
	 WDS.sampleResult.setResponseMessage(&quot;Page load error...&quot;)
}
else
{
	WDS.sampleResult.subSampleEnd(true)
}

//Click on &apos;Find Flights
var findflight=WDS.browser.findElement(pkg.By.xpath(&quot;//input[@value=&apos;Find Flights&apos;]&quot;))
WDS.sampleResult.subSampleStart(&quot;02_UI_FindFlights&quot;)
findflight.click()
if (WDS.browser.getTitle()!=&apos;BlazeDemo - reserve&apos;)
{
	WDS.sampleResult.subSampleEnd(false)
	WDS.sampleResult.setSuccessful(false)
	WDS.sampleResult.setResponseMessage(&quot;Reservation page not displayed....&quot;)
}
else
{
	WDS.sampleResult.subSampleEnd(true)
}

//Click on &apos;Choose This Flight
var choose=WDS.browser.findElement(pkg.By.xpath(&quot;//input[@value=&apos;Choose This Flight&apos;]&quot;))
WDS.sampleResult.subSampleStart(&quot;03_UI_ChooseThisFlight&quot;)
choose.click()
if (WDS.browser.getTitle()!=&apos;BlazeDemo Purchase&apos;)
{
	WDS.sampleResult.subSampleEnd(false)
	WDS.sampleResult.setSuccessful(false)
	WDS.sampleResult.setResponseMessage(&quot;Purchase Flights page not displayed...&quot;)
}
else
{
	WDS.sampleResult.subSampleEnd(true)
}

//Click on &apos;Purchase Flight&apos;
var purchase=WDS.browser.findElement(pkg.By.xpath(&quot;//input[@value=&apos;Purchase Flight&apos;]&quot;))
WDS.sampleResult.subSampleStart(&quot;04_UI_PurchaseFlight&quot;)
purchase.click()
if(WDS.browser.getTitle()!=&apos;BlazeDemo Confirmation&apos;)
{
	WDS.sampleResult.subSampleEnd(false)
	WDS.sampleResult.setSuccessful(false)	
	WDS.sampleResult.setResponseMessage(&quot;Confirmation page not displayed&quot;)
}
else
{
	WDS.sampleResult.subSampleEnd(true)
}
